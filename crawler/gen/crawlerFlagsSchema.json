{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "adId": {
            "type": "number"
        },
        "adUrlList": {
            "type": "string"
        },
        "chromeOptions": {
            "properties": {
                "executablePath": {
                    "type": "string"
                },
                "headless": {
                    "type": "boolean"
                },
                "profileDir": {
                    "type": "string"
                },
                "proxyServer": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "crawlId": {
            "type": "number"
        },
        "crawlName": {
            "type": "string"
        },
        "crawlOptions": {
            "properties": {
                "checkpointFreqSeconds": {
                    "type": "number"
                },
                "findAndCrawlArticlePage": {
                    "type": "boolean"
                },
                "findAndCrawlPageWithAds": {
                    "type": "number"
                },
                "refreshPage": {
                    "type": "boolean"
                },
                "shuffleCrawlList": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "jobId": {
            "type": "number"
        },
        "logLevel": {
            "enum": [
                1,
                2,
                3,
                4,
                5
            ],
            "type": "number"
        },
        "outputDir": {
            "type": "string"
        },
        "profileId": {
            "type": "string"
        },
        "profileOptions": {},
        "resumeIfAble": {
            "type": "boolean"
        },
        "scrapeOptions": {
            "properties": {
                "captureThirdPartyRequests": {
                    "type": "boolean"
                },
                "clickAds": {
                    "enum": [
                        "clickAndBlockLoad",
                        "clickAndScrapeLandingPage",
                        "noClick"
                    ],
                    "type": "string"
                },
                "scrapeAds": {
                    "type": "boolean"
                },
                "scrapeSite": {
                    "type": "boolean"
                },
                "screenshotAdsWithContext": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "url": {
            "type": "string"
        },
        "urlList": {
            "type": "string"
        }
    },
    "type": "object"
}

